{
  "name": "fluxo-caixa",
  "version": "1.0.0",
  "description": "Sistema de Fluxo de Caixa com Integração Open Finance e Agentes IA",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd src && npm run dev",
    "dev:backend": "cd server && npm run dev",
    "build": "cd src && npm run build",
    "install:all": "npm install && cd src && npm install && cd ../server && npm install"
  },
  "keywords": [
    "finance",
    "cashflow",
    "open-finance",
    "ai-agents"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}